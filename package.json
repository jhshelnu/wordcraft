{
    "devDependencies": {
        "daisyui": "^4.12.10",
        "tailwindcss": "^3.4.7"
    },
    "scripts": {
        "build": "npm run generateCss",
        "devBuild": "npm run generateCssAndWatch",
        "generateCss": "npx tailwindcss -i ./css/main.css -o ./static/main.css",
        "generateCssAndWatch": "npx tailwindcss -i ./css/main.css -o ./static/main.css --watch"
    }
}
